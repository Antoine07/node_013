<%- include('partials/header'); -%>
<div class="container">
    <div class="row">
        <div class="col-12">
            <p class="text-primary">
                <a type="submit" class="btn" href="/">Home</a>
            </p>
            <% if (kittens) { %>
                <h2>Students</h2>
                <ul>
                    <% for(const { name, age } of kittens) { %>
                     <li>Name : <%= name %> Age : <%= age %>
                    <form action="/delete" method="POST" >
                        <input type="hidden" name="name" value="<%= name %>" />
                        <button class="btn btn-danger">Delete</button>
                    </form>
                    </li>
                    <% }; %>
                  </ul>
              <% } %>
        </div>
    </div>
</div>
</div>
<%- include('partials/footer'); -%>

